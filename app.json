{
  "expo": {
    "name": "HW2P",
    "slug": "HW2P",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "privacy":"public",
    "githubUrl":"git@github.com:SitaraAlayev/HW2P-Final_Project.git",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow HW2P to access your photos.",
          "cameraPermission": "Allow HW2P to access your camera."

        }
      ]
    ]

  }
}
